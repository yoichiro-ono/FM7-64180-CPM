SECTBUF EQU     0FB00H          ;SECTOR BUFFER
BIOS2ST EQU     0FB00H          ;BIOS2 STACK

;-----------------------------------------------------------
;TIMER SETTING
;-----------------------------------------------------------
TICK_MS         EQU     10
INTERVAL        EQU     TICK_MS*400
INTERVALH       EQU     INTERVAL .shr. 8
INTERVALL       EQU     INTERVAL .and. 0ffh



